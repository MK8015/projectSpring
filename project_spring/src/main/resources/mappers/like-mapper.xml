<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mappers.like">

	<!-- 좋아요 등록 -->
	<insert id="insertLike">
		insert into t_like (member_id, product_id)
		values (#{member_id}, #{product_id})
	</insert>
		
	
</mapper>